<h1>Create A New Student</h1>
<%= form_for @student do |f| %>
  <%= f.label :name   %>
  <%= f.text_field :name%>
  <%= f.label :reading_proficiency  %>
  <%= f.number_field :reading_proficiency, in: 1..4%>
  <%= f.label :teacher %>
  <%= f.collection_select :teacher_id, Teacher.all, :id, :name %>
  <%= f.label :math_proficiency  %>
  <%= f.number_field :math_proficiency, in:1..4 %>
  <%= f.submit %>
<% end %>
